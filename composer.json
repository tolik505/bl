{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Application Template",
    "keywords": ["yii2", "framework", "advanced", "application template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "fxp/composer-asset-plugin":"~1.1.1",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "creocoder/yii2-translateable": "dev-master",
        "yiisoft/yii2-authclient": "*",
        "metalguardian/yii2-file-processor-module": "*",
        "vova07/yii2-imperavi-widget": "*",
        "metalguardian/yii2-form-builder": "~1.0",
        "metalguardian/yii2-datetime-picker-widget": "*",
        "notgosu/yii2-i18n-module": "dev-master",
        "notgosu/yii2-meta-tag-module": "*",
        "codemix/yii2-localeurls": "dev-master",
        "cosenary/Simple-PHP-Cache": "dev-master",
        "himiklab/yii2-sitemap-module": "^1.1",
        "kartik-v/yii2-widget-fileinput": "v1.0.0",
        "kartik-v/yii2-widget-select2": "v1.0.0",
        "bower-asset/cropper": "v0.7.9",
        "yiisoft/yii2-jui": "~2.0.0",
        "rmrevin/yii2-postman": "~2.2",
        "creocoder/yii2-nested-sets": "0.9.*",
        "justinvoelker/yii2-tagging": "*",
        "giovdk21/yii2-syntaxhighlighter": "dev-master",
        "ijackua/yii2-sharelinks-widget": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
